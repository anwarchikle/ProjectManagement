<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Projects__c</field>
        <field>Tasks__c</field>
        <field>Billable_Type__c</field>
        <field>Daily_Logs__c</field>
        <field>Time_Logs__c</field>
        <field>Date__c</field>
        <field>Start_Date__c</field>
        <field>End_Date__c</field>
        <field>Approval_status__c</field>
        <field>Rejection_Reason__c</field>
    </approvalPageFields>
    <description>This process is designed for controlling time logs filled by user.</description>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>Time log approval process</label>
    <processOrder>0</processOrder>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
